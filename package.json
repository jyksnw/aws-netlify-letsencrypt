{
  "name": "aws-netlify-ssl",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.206.0",
    "bluebird": "^3.5.1",
    "dotyaml": "^1.0.0",
    "letiny": "https://github.com/anatolsommer/letiny.git",
    "moment": "^2.21.0",
    "netlify": "^1.2.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "husky": "^0.14.3",
    "semistandard": "^12.0.1",
    "snyk": "^1.69.11"
  },
  "snyk": true,
  "scripts": {
    "run": "node index.js",
    "deploy": "semistandard --fix && sls deploy -v",
    "invoke": "sls invoke -f renew_certificate -l",
    "logs": "sls logs -f renew_certificate -l",
    "package": "semistandard --fix && sls package",
    "package-prod": "semistandard --fix && sls package -s production",
    "test": "semistandard --fix && depcheck . --ignores='husky',
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "husky": {
    "pre-commit": "npm run test"
  }
}
